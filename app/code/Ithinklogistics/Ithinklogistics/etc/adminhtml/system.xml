<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="ithinklogistics" translate="label" sortOrder="10">
            <label>Ithinklogistics</label>
        </tab>
        <section id="ithinklogistics" translate="label" sortOrder="130" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>separator-top</class>
            <label>Ithinklogistics</label>
            <tab>ithinklogistics</tab>
            <resource>Ithinklogistics_Ithinklogistics::ithinklogistics_config</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                <label>General Configuration</label>
                <field id="access_token" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Access Token</label>
                </field>
                <field id="secret_key" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Secret key</label>
                </field>
            </group>
        </section>
    </system>
</config>