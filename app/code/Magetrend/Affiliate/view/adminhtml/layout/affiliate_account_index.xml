        <css src="Magetrend_Affiliate::css/adminhtml.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magetrend\Affiliate\Block\Adminhtml\Account" name="adminhtml.affiliate.account.container"/>
